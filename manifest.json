{
    "manifest_version": 3,
    "name": "Schoology Rubric Scraper",
    "description": "Scrapes a given assignment's rubric scores",
    "version": "1.0",
    "action": {
      "default_title": "Click to scrape"
    },
    "background": {
      "service_worker": "scripts/background.js"
    },
    "permissions": ["activeTab", "scripting", "storage", "downloads"]
  }